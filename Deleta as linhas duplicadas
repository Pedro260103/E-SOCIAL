Sub a()

Dim TAMANHOExtracao As LongLong
Dim contador As LongLong
Dim LinhasExcluidas Como Inteiro
LinhasExcluidas = 0
contador = 2




'TAMANHOExtração = 32700 ' coloque o tamanho da sua tabela Extração


Do While contador <= TAMANHOExtração ' deleta as linhas vazias
 Se Range("AA" & contador). Valor = "" E Intervalo("Y" & contador). valor = "" então
 Linhas(contador). Excluir
 Linhas(contador). Excluir
        LinhasExcluidas = LinhasExcluidas + 2
        contador = contador + 1
 Mais
        contador = contador + 1
 Fim Se
    
Laço

MsgBox "Todas as " & LinhasExcluidas & " linhas foram excluídas"





Subfinal

